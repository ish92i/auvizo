{
  "mcp": {
    "context7": {
      "type": "local",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "enabled": true,
      "environment": {}
    },
    "convex": {
      "type": "local",
      "command": "npx",
      "args": ["-y", "convex@latest", "mcp", "start"],
      "enabled": true,
      "environment": {}
    },
    "shadcn": {
      "type": "local",
      "command": "npx",
      "args": ["-y", "shadcn@latest", "mcp"],
      "enabled": true,
      "environment": {}
    },
    "clerk": {
      "type": "remote",
      "url": "https://mcp.clerk.com/mcp",
      "enabled": true
    }
  }
}
